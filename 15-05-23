call:-
const obj = 
{
    name:"sumesh",
    last:"sharma",
    fulldata:function()
    {
        return this.name+" "+this.last;
    }
}
console.log(obj.fulldata());
const obj2  = 
{
    name:"Amit",
    last:"kumar"
}
const obj3 = 
{
    name:"rahul",
    last:"Dev"
}
console.log(obj.fulldata.call(obj2));
console.log(obj.fulldata.call(obj3));
// apply:-


